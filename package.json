  "main": "dist/index.js",
  "scripts": {
    "build": "tslint 'src/**/*.ts' && tsc",
    "build-lambda": "mkdir lambda && cp -r dist/ lambda && cp package.json lambda && cd lambda; NODE_ENV=production npm i && zip -r lambda.zip *; mv lambda.zip ../; cd ../; rm -rf lambda",
    "pretest": "npm run build",
    "serve": "npm run build && NODE_ENV=production node dist/index.js",
    "start": "npm run build -- -w",
    "test": "NODE_ENV=test mocha -w test/"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "figlet": "^1.2.0",
    "lodash": "^4.17.4",
