    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "@types/redis": "^2.8.4",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "mocha": "^5.0.0",
    "sinon": "^4.2.2",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
